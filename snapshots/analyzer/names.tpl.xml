<analyzer-snapshots>
    <!-- Ambiguous column a from A and B -->
    <analyzer-snapshot name="names_columnref_ambiguous">
        <catalog>
            <script>
                <input>
                    create table A (a int);
                    create table B (a int);
                </input>
            </script>  
        </catalog>
        <script>  
            <input>
                select * from A, B where a = 2;
            </input>
        </script>  
    </analyzer-snapshot>
    <!-- Subquery scope: a resolves to B only -->
    <analyzer-snapshot name="names_columnref_ambiguous_scope_overrules">
        <catalog>
            <script>
                <input>
                    create table A (a int);
                    create table B (a int);
                </input>
            </script>  
        </catalog>
        <script>  
            <input>
                select * from A, (select * from B where a = 2);
            </input>
        </script>  
    </analyzer-snapshot>
    <!-- Outer a=3, inner a=2: correctly scoped -->
    <analyzer-snapshot name="names_columnref_ambiguous_correctly_scoped">
        <catalog>
            <script>
                <input>
                    create table A (a int);
                    create table B (a int);
                </input>
            </script>  
        </catalog>
        <script>  
            <input>
                select * from A, (select * from B where a = 2) c where a = 3;
            </input>
        </script>  
    </analyzer-snapshot>
    <!-- Duplicate table ref: FROM A, A -->
    <analyzer-snapshot name="names_duplicate_tableref">
        <catalog>
            <script>
                <input>
                    create table A (a int);
                </input>
            </script>  
        </catalog>
        <script>  
            <input>
                select * from A, A;
            </input>
        </script>  
    </analyzer-snapshot>
    <!-- Duplicate table: A vs "A" different casing -->
    <analyzer-snapshot name="names_duplicate_tableref_different_casing">
        <catalog>
            <script>
                <input>
                    create table A (a int);
                    create table "A" (a int);
                </input>
            </script>  
        </catalog>
        <script>  
            <input>
                select * from "A", A;
            </input>
        </script>  
    </analyzer-snapshot>
    <!-- Duplicate: "a" vs A (quoted vs unquoted) -->
    <analyzer-snapshot name="names_duplicate_tableref_colliding_with_quotes">
        <catalog>
            <script>
                <input>
                    create table "a" (a int);
                </input>
            </script>  
        </catalog>
        <script>  
            <input>
                select * from "a", A;
            </input>
        </script>  
    </analyzer-snapshot>
    <!-- Table name used as implicit alias: foo.a -->
    <analyzer-snapshot name="names_tablename_as_alias">
        <catalog>
            <script>
                <input>
                    create table foo(a int);
                </input>
            </script>  
        </catalog>
        <script>  
            <input>
                select * from foo where foo.a = 1;
            </input>
        </script>  
    </analyzer-snapshot>
    <!-- foo.a with cross join: alias a and table foo -->
    <analyzer-snapshot name="names_tablename_as_alias_with_cross_join">
        <catalog>
            <script>
                <input>
                    create table foo(a int);
                </input>
            </script>  
        </catalog>
        <script>  
            <input>
                select * from foo a, foo where foo.a = 1;
            </input>
        </script>  
    </analyzer-snapshot>
    <!-- Ambiguous foo: three refs (x, foo, foo), foo.a -->
    <analyzer-snapshot name="names_tablename_as_alias_but_duplicate">
        <catalog>
            <script>
                <input>
                    create table foo(a int);
                </input>
            </script>  
        </catalog>
        <script>  
            <input>
                select * from foo x, foo, foo where foo.a = 1;
            </input>
        </script>  
    </analyzer-snapshot>
    <!-- Unresolved: foo x then foo.a (table name as alias but alias is x) -->
    <analyzer-snapshot name="names_unresolved_tablename_as_alias_with_tableref_alias">
        <catalog>
            <script>
                <input>
                    create table foo(a int);
                </input>
            </script>  
        </catalog>
        <script>  
            <input>
                select * from foo x where foo.a = 1;
            </input>
        </script>  
    </analyzer-snapshot>
</analyzer-snapshots>
