<analyzer-snapshots>
    <!-- Analyze filter with variable comparison -->
    <analyzer-snapshot name="filter_variable">
        <catalog />
        <script>
            <input>
                select * from foo where a &lt; 3
            </input>
        </script>
    </analyzer-snapshot>
    <!-- Analyze time range filter with interval -->
    <analyzer-snapshot name="filter_time">
        <catalog />
        <script>
            <input>
                select * from foo where event_timestamp > current_timestamp - interval '30 days'
            </input>
        </script>
    </analyzer-snapshot>
    <!-- Analyze filter with current_timestamp - interval -->
    <analyzer-snapshot name="filter_time_minus_interval">
        <catalog />
        <script>
            <input>
                select * from foo where event_timestamp > current_timestamp - interval '30 days'
            </input>
        </script>
    </analyzer-snapshot>
</analyzer-snapshots>
