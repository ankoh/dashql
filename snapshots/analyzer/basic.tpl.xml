    <!-- Column refs a, b from A and B in join -->
    <analyzer-snapshot name="refs_0">
        <catalog>
            <script>  
                <input>
                    create table A (a int, b int);
                    create table B (d int, e int);
                </input>
            </script>  
        </catalog>
        <script>  
            <input>
                select * from A, B where a = b;
            </input>
        </script>  
    </analyzer-snapshot>
    <!-- Join edges: a+d=e -->
    <analyzer-snapshot name="edges_0">
        <catalog>
            <script>  
                <input>
                    create table A (a int, b int);
                    create table B (d int, e int);
                </input>
            </script>  
        </catalog>
        <script>
            <input>
                select * from A, B where a + d = e;
            </input>
        </script>  
    </analyzer-snapshot>
    <!-- Join edges: a=d and b=e -->
    <analyzer-snapshot name="edges_1">
