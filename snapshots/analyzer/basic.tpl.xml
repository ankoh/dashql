        </catalog>
        <script>  
            <input>
                select * from A, B where a = (d + 42);
            </input>
        </script>  
    </analyzer-snapshot>
    <!-- Join edge: INNER JOIN ... ON (a = b) -->
    <analyzer-snapshot name="edges_4">
        <catalog>
            <script>  
                <input>
                    create table A (a int, b int);
                    create table B (d int, e int);
                </input>
            </script>
        </catalog>
        <script>  
            <input>
                select * from A join B on (a = b);
            </input>
        </script>  
    </analyzer-snapshot>
