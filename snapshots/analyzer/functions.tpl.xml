    <analyzer-snapshot name="func_regexp_extract">
        <catalog />
        <script>
            <input>
                select regexp_extract(a, '[0-9a-zA-Z]+\.[0-9a-zA-Z]+\.([0-9a-zA-Z]+)', 1) from foo
            </input>
        </script>
    </analyzer-snapshot>
    <!-- Analyze time filter with current_timestamp - interval -->
