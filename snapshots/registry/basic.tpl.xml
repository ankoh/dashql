<registry-snapshots>
    <registry-snapshot name="simple_1">
        <catalog>
            <script>
                <input>
                    create table foo (a int);
                </input>
            </script>
        </catalog>
        <registry>
            <script>
                <input>
                    select * from foo where a &lt; 6
                </input>
            </script>
            <script>
                <input>
                    select * from foo where (a + 1) &lt; 8
                </input>
            </script>
        </registry>
    </registry-snapshot>
</registry-snapshots>
