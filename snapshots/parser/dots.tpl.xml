<parser-snapshots>
    <!-- Parse qualified name with space after dot (dbname. foo) -->
    <parser-snapshot name="dot_test_space_ident">
        <input>
            select * from dbname. foo
        </input>
    </parser-snapshot>
    <!-- Parse qualified name with keyword after dot (dbname. where) -->
    <parser-snapshot name="dot_test_space_keyword">
        <input>
            select * from dbname.  where
        </input>
    </parser-snapshot>
    <!-- Parse qualified name with dot at EOF -->
    <parser-snapshot name="dot_test_eof">
        <input>
            select * from dbname.
        </input>
    </parser-snapshot>
    <!-- Recovery: dbname. where a = b (dot then keyword) -->
    <parser-snapshot name="dot_where_recovery">
        <input>
            select * from dbname. where a = b
        </input>
    </parser-snapshot>
</parser-snapshots>
