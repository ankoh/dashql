<completion-snapshots>
    <completion-snapshot name="empty">
        <script context="main" />
        <cursor context="main">
            <search text="" index="0" />
        </cursor>
        <completions limit="3" />
    </completion-snapshot>
    <completion-snapshot name="complete_table_name">
        <script context="external">
            create table foo(a int);
        </script>
        <script context="main">
            select * from fo
        </script>
        <cursor context="main">
            <search text="fo" index="2" />
        </cursor>
        <completions limit="3" />
    </completion-snapshot>
    <completion-snapshot name="dont_complete_at_lrb">
        <script context="external">
            create table foo(a int);
        </script>
        <script context="main">
            select * from ( foo );
        </script>
        <cursor context="main">
            <search text="from (" index="6" />
        </cursor>
        <completions limit="3" />
    </completion-snapshot>
    <completion-snapshot name="table_name_resolving_column">
        <script context="external">
create table table1(attr1 integer);
create table table2(attr2 integer);
create table table3(attr3 integer);
        </script>
        <script context="main">
            select attr2 from tab
        </script>
        <cursor context="main">
            <search text="tab" index="3" />
        </cursor>
        <completions limit="3" />
    </completion-snapshot>
</completion-snapshots>
