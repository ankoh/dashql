<formatter-snapshots>
    <formatter-snapshot name="prec_plus">
        <input>
            select 1+2
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
    <formatter-snapshot name="prec_plus_mul_1">
        <input>
            select 1+2*3
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
    <formatter-snapshot name="prec_plus_mul_2">
        <input>
            select (1+2)*3
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
    <formatter-snapshot name="prec_plus_mul_3">
        <input>
            select 1*2+3
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
    <!-- AND has higher precedence than OR -->
    <formatter-snapshot name="prec_and_or_1">
        <input>
            select 1 or 2 and 3
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
    <formatter-snapshot name="prec_and_or_2">
        <input>
            select (1 or 2) and 3
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
    <!-- Left-associative minus -->
    <formatter-snapshot name="prec_minus_left">
        <input>
            select 1-2-3
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
    <formatter-snapshot name="prec_minus_paren">
        <input>
            select 10-(2-3)
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
    <!-- Unary minus: operand gets parens (n==1) -->
    <formatter-snapshot name="prec_unary_minus_1">
        <input>
            select -1+2
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
    <formatter-snapshot name="prec_unary_minus_2">
        <input>
            select -(1+2)
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
    <!-- Comparison (level 6) higher than AND (level 4): parens around comparison when under AND -->
    <formatter-snapshot name="prec_compare_and">
        <input>
            select 1&gt;2 and 3=3
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
    <!-- Division and multiply same precedence (left-assoc) -->
    <formatter-snapshot name="prec_div_mul">
        <input>
            select 12/3*2
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
    <formatter-snapshot name="prec_mul_div">
        <input>
            select (12/3)*2
        </input>
        <formatted indent="2" />
    </formatter-snapshot>
</formatter-snapshots>
