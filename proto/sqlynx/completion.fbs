namespace sqlynx.proto;

table CompletionCandidate {
    /// The completion
    name_text: string;
    /// The tags
    name_tags: uint64;
    /// The score
    score: uint32;
    /// Is the candidate in statement?
    in_statement: bool;
}

table Completion {
    /// The text offset
    text_offset: uint32;
    /// The completion candidates
    candidates: [CompletionCandidate];
}
