namespace flatsql.proto;

table ScriptCursorInfo {
    /// The text offset
    text_offset: uint32;
    /// The scanner token id
    scanner_token_id: uint32;
    /// The AST node id
    ast_node_id: uint32;
    /// The statement id
    statement_id: uint32;
    /// The query edge id
    query_edge_id: uint32;
    /// The table id
    table_id: uint32;
}
