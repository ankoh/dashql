namespace flatsql.proto;

table CompletionCandidate {
    // The name id
    name_id: uint64;
    /// The tags
    tags: uint64;
    /// The completion
    text: string;
    /// The score
    score: uint32;
}

table Completion {
    /// The text offset
    text_offset: uint32;
    /// The token that gets completed
    scanner_token_id: uint32;
    /// The completion candidates
    candidates: [CompletionCandidate];
}
