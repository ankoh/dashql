namespace flatsql.proto;

table CompletionOption {
    // The context id
    context_id: uint32;
    /// The tags
    tags: uint64;
    /// The completion
    text: string;
    /// The score
    score: double;
}

table Completion {
    /// The text offset
    text_offset: uint32;
    /// The token that gets completed
    scanner_token_id: uint32;
    /// The completion entries
    entries: [CompletionOption];
}
