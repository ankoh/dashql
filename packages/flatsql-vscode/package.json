{
	"name": "flatsql-vscode",
	"version": "0.0.1",
	"displayName": "flatsql",
	"description": "SQL completion, navigation, and more",
	"license": "MPL-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ankoh/flatsql"
	},
	"private": true,
	"engines": {
		"vscode": "^1.80.2"
	},
	"activationEvents": [
		"onLanguage:sql"
	],
	"categories": [
		"Programming Languages",
		"Linters",
		"Formatters"
	],
	"keywords": [
		"SQL"
	],
	"main": "./dist/extension",
	"scripts": {
		"vscode:prepublish": "yarn run build",
		"build": "tsc -b",
		"watch": "tsc -b -w",
		"package": "vsce package --yarn"
	},
	"dependencies": {
		"@ankoh/flatsql-lsp": "link:../flatsql-lsp",
		"vscode-languageclient": "^8.1.0"
	},
	"devDependencies": {
		"@types/node": "^18.17.1",
		"@types/vscode": "^1.80.0",
		"typescript": "^5.1.6",
		"@vscode/vsce": "^2.20.0"
	}
}
