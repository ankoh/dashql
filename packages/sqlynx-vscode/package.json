{
	"name": "sqlynx-vscode",
	"version": "0.0.1",
	"displayName": "sqlynx",
	"description": "SQL completion, navigation, and more",
	"license": "MPL-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ankoh/sqlynx"
	},
	"private": true,
	"engines": {
		"vscode": "^1.85.1"
	},
	"activationEvents": [
		"onLanguage:sql"
	],
	"categories": [
		"Programming Languages",
		"Linters",
		"Formatters"
	],
	"keywords": [
		"SQL"
	],
	"main": "./dist/extension",
	"scripts": {
		"vscode:prepublish": "yarn run build",
		"build": "tsc -b",
		"watch": "tsc -b -w",
		"package": "vsce package --yarn"
	},
	"dependencies": {
		"@ankoh/sqlynx-lsp": "link:../sqlynx-lsp",
		"vscode-languageclient": "^9.0.1"
	},
	"devDependencies": {
		"@types/node": "^20.10.4",
		"@types/vscode": "^1.85.0",
		"typescript": "^5.3.3",
		"@vscode/vsce": "^2.22.0"
	}
}
