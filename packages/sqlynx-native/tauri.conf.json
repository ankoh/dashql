            "./icons/icon.ico",
            "./icons/icon.png",
            "./icons/16x16.png",
            "./icons/16x16@2x.png",
            "./icons/32x32.png",
            "./icons/32x32@2x.png",
            "./icons/64x64.png",
            "./icons/64x64@2x.png",
            "./icons/128x128.png",
            "./icons/128x128@2x.png",
            "./icons/256x256.png",
            "./icons/256x256@2x.png",
            "./icons/512x512.png",
            "./icons/512x512@2x.png"
