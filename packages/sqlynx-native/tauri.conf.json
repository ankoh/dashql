    "build": {
        "beforeDevCommand": "yarn workspace @ankoh/sqlynx-app run serve:dev",
        "devUrl": "http://localhost:9002",
        "frontendDist": "../sqlynx-app/build/reloc"
    },
    "bundle": {
        "active": true,
        "category": "Utility",
        "copyright": "",
        "externalBin": [],
        "icon": [
